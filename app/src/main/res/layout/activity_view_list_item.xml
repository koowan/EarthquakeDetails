<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".ViewListItem">

    <TextView
        android:id="@+id/titleHeader"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:background="@color/textview"
        android:text="Title"
        android:textSize="18dp"
        tools:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/titleText"
        android:layout_width="400dp"
        android:layout_centerHorizontal="true"
        android:layout_height="50dp"
        android:layout_marginTop="40dp"
        android:background="@color/textview"
        tools:textColor="@color/textColor"
        android:text="" />

    <TextView
        android:id="@+id/descriptionHeader"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="90dp"
        android:background="@color/textview"
        android:text="Description"
        android:textSize="18dp"
        tools:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/descriptionText"
        android:layout_width="400dp"
        android:layout_centerHorizontal="true"
        android:layout_height="70dp"
        android:layout_marginTop="114dp"
        android:background="@color/textview"
        tools:textColor="@color/textColor"
        android:text="" />

    <TextView
        android:id="@+id/pubDateHeader"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="184dp"
        android:background="@color/textview"
        android:text="Publish Date"
        android:textSize="18dp"
        tools:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/pubDateText"
        android:layout_width="400dp"
        android:layout_centerHorizontal="true"
        android:layout_height="30dp"
        android:layout_marginTop="208dp"
        android:background="@color/textview"
        tools:textColor="@color/textColor"
        android:text="" />

    <TextView
        android:id="@+id/categoryHeader"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="238dp"
        android:background="@color/textview"
        android:text="Category"
        android:textSize="18dp"
        tools:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/categoryText"
        android:layout_width="400dp"
        android:layout_centerHorizontal="true"
        android:layout_height="30dp"
        android:layout_marginTop="262dp"
        android:background="@color/textview"
        tools:textColor="@color/textColor"
        android:text="" />

    <TextView
        android:id="@+id/geoLatHeader"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="292dp"
        android:background="@color/textview"
        android:text="Latitude"
        android:textSize="18dp"
        tools:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/geoLatText"
        android:layout_width="400dp"
        android:layout_centerHorizontal="true"
        android:layout_height="20dp"
        android:layout_marginTop="316dp"
        android:background="@color/textview"
        tools:textColor="@color/textColor"
        android:text="" />

    <TextView
        android:id="@+id/geoLongHeader"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="336dp"
        android:background="@color/textview"
        android:text="Longitude"
        android:textSize="18dp"
        tools:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/geoLongText"
        android:layout_width="400dp"
        android:layout_centerHorizontal="true"
        android:layout_height="25dp"
        android:layout_marginTop="360dp"
        android:background="@color/textview"
        tools:textColor="@color/textColor"
        android:text="" />

    <TextView
        android:id="@+id/linkHeader"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="385dp"
        android:background="@color/textview"
        android:text="Link To Webpage"
        android:textSize="18dp"
        tools:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/linkText"
        android:layout_width="400dp"
        android:layout_centerHorizontal="true"
        android:layout_height="35dp"
        android:layout_marginTop="409dp"
        android:background="@color/textview"
        tools:textColor="@color/colorPrimaryDark"
        android:text=""
        android:linksClickable="true"/>




</RelativeLayout>